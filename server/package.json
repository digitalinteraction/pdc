{
  "name": "@openlab/pdc-server",
  "type": "module",
  "version": "1.11.2",
  "scripts": {
    "build": "tsc",
    "dev": "node --loader=ts-node/esm --require dotenv/config src/cli.ts",
    "debug": "node --inspect-brk --loader=ts-node/esm --require dotenv/config src/cli.ts",
    "redis": "docker-compose exec redis redis-cli",
    "serve": "npm run -s dev serve -- --settings",
    "schedule": "npm run -s dev fetch-schedule > notion.json",
    "schedule:local": "npm run -s dev fetch-schedule -- --localFile notion.json"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.10.2",
    "@koa/cors": "^3.3.0",
    "@koa/router": "^10.1.1",
    "@notionhq/client": "^1.0.4",
    "@openlab/deconf-api-toolkit": "^4.8.1",
    "@openlab/deconf-shared": "^3.5.0",
    "@sendgrid/mail": "^7.6.2",
    "@socket.io/redis-adapter": "^7.1.0",
    "debug": "^4.3.4",
    "dedent": "^0.7.0",
    "dotenv": "^16.0.0",
    "got": "^12.1.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.1.0",
    "koa-json": "^2.0.2",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "markdown-it": "^13.0.1",
    "minio": "^7.0.29",
    "ms": "^2.1.3",
    "redis": "^3.1.2",
    "socket.io": "^4.4.1",
    "superstruct": "^0.15.4",
    "un": "^0.0.0",
    "yaml": "^2.0.0",
    "yargs": "^17.4.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/dedent": "^0.7.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/koa__cors": "^3.1.1",
    "@types/koa__router": "^8.0.11",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-json": "^2.0.20",
    "@types/koa-mount": "^4.0.1",
    "@types/koa-static": "^4.0.2",
    "@types/markdown-it": "^12.2.3",
    "@types/minio": "^7.0.13",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.23",
    "@types/svgo": "^2.6.3",
    "@types/yargs": "^17.0.10",
    "svgo": "^2.8.0",
    "typescript": "^4.6.4"
  }
}
